<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:expandable="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">
    <include
        layout="@layout/common_toolbar"
        />
    <ScrollView
        android:paddingTop="10dp"
        android:clipToPadding="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_border_layout"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:text="企业名称"
                        android:textColor="@color/base_text_color"
                        android:textSize="@dimen/font_size_16" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:text="深圳中商产业研究院有限公司"
                        android:textColor="@color/title_color"
                        android:textSize="@dimen/font_size_16" />
                </LinearLayout>

                <View style="@style/MyHorizontalDividerLine" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:text="注册号"
                        android:textColor="@color/base_text_color"
                        android:textSize="@dimen/font_size_16" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:text="201554854545"
                        android:textColor="@color/title_color"
                        android:textSize="@dimen/font_size_16" />
                </LinearLayout>

                <View style="@style/MyHorizontalDividerLine" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:text="企业通信地址"
                        android:textColor="@color/base_text_color"
                        android:textSize="@dimen/font_size_16" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:text="深圳市前海深港合作区前海一路1号A栋201室(入驻深圳市前海商务秘书有限公司)"
                        android:textColor="@color/title_color"
                        android:textSize="@dimen/font_size_16" />
                </LinearLayout>

                <View style="@style/MyHorizontalDividerLine" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:text="电子邮箱"
                        android:textColor="@color/base_text_color"
                        android:textSize="@dimen/font_size_16" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:text="google@sina.com"
                        android:textColor="@color/title_color"
                        android:textSize="@dimen/font_size_16" />
                </LinearLayout>

                <View style="@style/MyHorizontalDividerLine" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:text="邮政编码"
                        android:textColor="@color/base_text_color"
                        android:textSize="@dimen/font_size_16" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:text="431900"
                        android:textColor="@color/title_color"
                        android:textSize="@dimen/font_size_16" />
                </LinearLayout>

                <View style="@style/MyHorizontalDividerLine" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:text="企业经营状态"
                        android:textColor="@color/base_text_color"
                        android:textSize="@dimen/font_size_16" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:text="在业"
                        android:textColor="@color/title_color"
                        android:textSize="@dimen/font_size_16" />
                </LinearLayout>

                <View style="@style/MyHorizontalDividerLine" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:text="从业人数（人）"
                        android:textColor="@color/base_text_color"
                        android:textSize="@dimen/font_size_16" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:text="不公示"
                        android:textColor="@color/title_color"
                        android:textSize="@dimen/font_size_16" />
                </LinearLayout>
            </LinearLayout>

            <com.qihaosou.view.ExpandableLayout
                android:id="@+id/el_reportdetial_assetdata"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5.0dip"
                android:layout_marginTop="5.0dip"
                android:layout_marginRight="5.0dip"
                android:background="@drawable/bg_border_layout"
                expandable:el_contentLayout="@layout/view_content"
                expandable:el_headerLayout="@layout/view_header" />
            <com.qihaosou.view.ExpandableLayout
                android:id="@+id/el_reportdetial_partners"
                android:background="@drawable/bg_border_layout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5.0dip"
                android:layout_marginTop="5.0dip"
                android:layout_marginRight="5.0dip"
                expandable:el_contentLayout="@layout/view_content"
                expandable:el_headerLayout="@layout/view_header" />
        </LinearLayout>
    </ScrollView>


</LinearLayout>
